{
  "name": "Chrome GitHub",
  "version": "0",
  "icons": {
    "128": "img/icon-128.png",
    "34": "img/icon-34.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "content_scripts": [
    {
      "js": ["projects.js"],
      "matches": ["*://github.com/*/*/projects/*"],
      "run_at": "document_idle"
    }
  ],
  "permissions": ["*://github.com/*/*/projects/*", "tabs", "webNavigation"]
}
